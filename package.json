{
  "name": "front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently -n vue,api \"yarn --cwd ./frontend serve --silent\" \"yarn --cwd ./api-nestjs start:dev\" ",
    "install:api-vue":"concurrently -n api,vue \"npm --cwd ./api-nestjs install\" \"npm --cwd ./frontend install\" ",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "concurrently": "^5.3.0"
  }
}
